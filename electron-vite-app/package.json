{
  "name": "electron-vite-app",
  "version": "1.0.0",
  "description": "description",
  "main": "dist/main/index.cjs",
  "scripts": {
    "pack": "cross-env NPM_CONFIG_ELECTRON_MIRROR=http://npm.taobao.org/mirrors/electron/ ELECTRON_BUILDER_BINARIES_MIRROR=http://npm.taobao.org/mirrors/electron-builder-binaries/  electron-builder build --config electron-builder.config.js --dir",
    "prepack": "npm run build",
    "dev": "node scripts/dev.js",
    "build": "npm run check && node scripts/build.js",
    "check": "npm run check:main && npm run check:renderer:main",
    "check:main": "tsc --noEmit -p src/main/tsconfig.json",
    "check:renderer:main": "vue-tsc --noEmit -p src/renderer/main/tsconfig.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browserslist": [
    "Chrome 96"
  ],
  "dependencies": {
    "vue": "^3.2.22"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.9.4",
    "cross-env": "^7.0.3",
    "electron": "^16.0.1",
    "electron-builder": "^22.14.5",
    "typescript": "^4.5.2",
    "vite": "^2.6.14",
    "vue-tsc": "^0.29.6"
  }
}
